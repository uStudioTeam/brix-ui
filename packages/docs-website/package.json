{
  "name": "docs-website",
  "version": "1.0.0",
  "author": "uStudio Front-end Department",
  "repository": "https://github.com/uStudioCompany/ustudio-ui",
  "homepage": "https://ustudiocompany.github.io/ustudio-ui/",
  "scripts": {
    "clean:dist": "rimraf dist",
    "dev": "next",
    "build": "yarn clean:dist && next build && next export -o dist && copyfiles .nojekyll dist",
    "deploy": "yarn build && gh-pages -b gh-pages --dotfiles=true -d dist"
  },
  "dependencies": {
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.10.7",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "next": "^9.2.1",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-element-to-jsx-string": "^14.3.1",
    "react-markdown": "^4.3.1",
    "react-syntax-highlighter": "^12.2.1",
    "styled-components": "^5.0.1",
    "typescript": "^3.7.5",
    "ustudio-ui": "^1.2.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "(pages|components|shared|utils)/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT"
}
